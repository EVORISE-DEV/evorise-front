{
  "compilerOptions": {
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "strict": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "allowImportingTsExtensions": true,
    "noEmit": true,
    "jsx": "react",
    "target": "es2016",
    "module": "commonjs",
    "baseUrl": "./",
    "paths": {
      "@assets/*": ["src/assets/*"]
    }
  },
  "include": [
    "src/**/*"
  ]
}
